<ul>
	<% for user in @users %>
      <li><%=h user.last_name %>, <%=h user.first_name %>
	<%=h user.network_relationship %> <% if user.role == 'ADMIN'%> <%= link_to "Remove", register_path %> <% end %>
	<% if user.role == 'ADMIN' %>
		<input type="checkbox" checked="true">Coordinate?</input>
	<% else %>
		<input type="checkbox" checked="false">Coordinate?</input>
	<% end %>
	</li>
    <% end %>	
</ul>